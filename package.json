{
  "name": "@elizaos/eliza-starter",
  "version": "0.1.9",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsx src/index.ts",
    "clean": "./scripts/clean.sh",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@1inch/byte-utils": "^3.0.0",
    "@1inch/limit-order-sdk": "^4.11.6",
    "@chainsafe/libp2p-gossipsub": "^14.1.0",
    "@chainsafe/libp2p-noise": "^16.0.0",
    "@chainsafe/libp2p-yamux": "^7.0.0",
    "@elizaos/adapter-postgres": "^0.1.9",
    "@elizaos/adapter-sqlite": "0.1.9",
    "@elizaos/client-direct": "0.1.9",
    "@elizaos/client-telegram": "0.1.9",
    "@elizaos/core": "0.1.9",
    "@elizaos/plugin-bootstrap": "0.1.9",
    "@goat-sdk/core": "^0.4.6",
    "@goat-sdk/wallet-evm": "^0.2.8",
    "@goat-sdk/wallet-lit": "^0.2.15",
    "@goat-sdk/wallet-solana": "^0.2.13",
    "@goat-sdk/wallet-viem": "^0.2.7",
    "@libp2p/bootstrap": "^11.0.19",
    "@libp2p/identify": "^3.0.1",
    "@libp2p/interface": "^2.4.1",
    "@libp2p/kad-dht": "^14.2.2",
    "@libp2p/ping": "^2.0.17",
    "@libp2p/tcp": "^10.0.17",
    "@lit-protocol/agent-wallet": "0.1.0-19",
    "@lit-protocol/aw-signer": "0.1.0-19",
    "@lit-protocol/constants": "^7.0.5",
    "@lit-protocol/lit-node-client": "^7.0.6",
    "@lit-protocol/types": "^7.0.6",
    "@lit-protocol/wrapped-keys": "^7.0.6",
    "amqplib": "0.10.5",
    "axios": "^1.7.9",
    "better-sqlite3": "11.5.0",
    "fastembed": "^1.14.1",
    "fs": "0.0.1-security",
    "libp2p": "^2.0.0",
    "moment": "^2.30.1",
    "net": "1.0.2",
    "patch-package": "^8.0.0",
    "path": "0.12.7",
    "readline": "1.3.0",
    "sharp": "^0.33.5",
    "uint8arrays": "^5.1.0",
    "url": "0.11.4",
    "viem": "^2.22.21",
    "ws": "8.18.0",
    "yargs": "17.7.2",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0",
      "sqlite-vec": "0.1.7-alpha.2"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "onnxruntime-node",
      "sharp"
    ],
    "patchedDependencies": {
      "@goat-sdk/wallet-lit": "patches/@goat-sdk__wallet-lit.patch"
    }
  },
  "devDependencies": {
    "pm2": "5.4.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "tsx": "^4.19.2",
    "typescript": "5.6.3"
  }
}
